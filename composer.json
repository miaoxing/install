{
  "name": "miaoxing/install",
  "type": "miaoxing-plugin",
  "description": "Miaoxing Install Plugin",
  "keywords": [
    "miaoxing",
    "install"
  ],
  "license": "MIT",
  "require": {
    "miaoxing/plugin": "^0.13.2",
    "miaoxing/services": "^0.8.1",
    "miaoxing/admin": "^0.11.2"
  },
  "require-dev": {
    "miaoxing/dev": "^8.2.3"
  },
  "autoload": {
    "psr-4": {
      "Miaoxing\\Install\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MiaoxingTest\\Install\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "composer/installers": true,
      "phpstan/extension-installer": true
    }
  },
  "extra": {
    "wei-preload": [
      "install"
    ],
    "require-ci": {
      "wei/wei": "twinh/wei as 0.15.x-dev",
      "miaoxing/dev": "miaoxing/dev as 8.x-dev",
      "miaoxing/phpstan-miaoxing": "miaoxing/phpstan-miaoxing as 1.x-dev",
      "miaoxing/plugin": "miaoxing/plugin as 0.13.x-dev",
      "miaoxing/services": "miaoxing/services as 0.8.x-dev"
    }
  }
}
