{
  "name": "miaoxing/install",
  "type": "miaoxing-plugin",
  "description": "Miaoxing Install Plugin",
  "keywords": [
    "miaoxing",
    "install"
  ],
  "license": "MIT",
  "require": {
    "miaoxing/plugin": "^0.17.2",
    "miaoxing/services": "^0.9.10",
    "miaoxing/admin": "^0.17.0"
  },
  "require-dev": {
    "miaoxing/dev": "^9.3.0"
  },
  "autoload": {
    "psr-4": {
      "Miaoxing\\Install\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MiaoxingTest\\Install\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "composer/installers": true,
      "phpstan/extension-installer": true
    }
  },
  "extra": {
    "wei-preload": [
      "install"
    ],
    "require-ci": {
      "wei/wei": "twinh/wei as 0.18.x-dev",
      "miaoxing/dev": "miaoxing/dev as 9.x-dev",
      "miaoxing/phpstan-miaoxing": "miaoxing/phpstan-miaoxing as 3.x-dev",
      "miaoxing/plugin": "miaoxing/plugin as 0.17.x-dev",
      "miaoxing/services": "miaoxing/services as 0.9.x-dev"
    }
  },
  "scripts": {
    "post-update-cmd": [
      "@php wei g:auto-completion"
    ]
  }
}
