{
  "name": "miaoxing/install",
  "type": "miaoxing-plugin",
  "description": "Miaoxing Install Plugin",
  "keywords": [
    "miaoxing",
    "install"
  ],
  "license": "MIT",
  "require": {
    "miaoxing/plugin": "dev-master@dev",
    "miaoxing/services": "dev-master@dev"
  },
  "require-dev": {
    "miaoxing/coding-standards": "^4.1.1"
  },
  "autoload": {
    "psr-4": {
      "Miaoxing\\Install\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MiaoxingTest\\Install\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "installer-types": [
      "miaoxing-plugin"
    ],
    "installer-paths": {
      "plugins/{$name}/": [
        "type:miaoxing-plugin"
      ]
    },
    "branch-alias": {
      "dev-master": "2.0.x-dev"
    }
  }
}
