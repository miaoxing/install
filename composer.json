{
  "name": "miaoxing/install",
  "type": "miaoxing-plugin",
  "description": "Miaoxing Install Plugin",
  "keywords": [
    "miaoxing",
    "install"
  ],
  "license": "MIT",
  "require": {
    "miaoxing/plugin": "^0.3.1",
    "miaoxing/services": "^0.3.1"
  },
  "require-dev": {
    "miaoxing/dev": "^6.2.0"
  },
  "autoload": {
    "psr-4": {
      "Miaoxing\\Install\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MiaoxingTest\\Install\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "installer-types": [
      "miaoxing-plugin"
    ],
    "installer-paths": {
      "plugins/{$name}/": [
        "type:miaoxing-plugin"
      ]
    },
    "wei-preload": [
      "install"
    ]
  }
}
